    "@codemirror/view": "^6.38.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/three": "^0.178.0",
    "@whttlr/ui-theme": "workspace:*",
    "antd": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.263.1",
